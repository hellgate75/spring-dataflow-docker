#!/bin/sh
function usage() {
  echo "$SCRIPT_NAME --debug|--help|-h"
  echo "   [--debug]     Activate debug mode in Spring logs"
  echo "   [--help][-h]  Print current script usage help"
}

if [ '-h' = "$1" ]; then
   echo -e "$(usage)"
   exit 0
fi
if [ '--help' = "$1" ]; then
   echo -e "$(usage)"
   exit 0
fi
